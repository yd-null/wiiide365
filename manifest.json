{
    "manifest_version": 3,
    "name": "wiiide365",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "arrow-128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://*.crm6.dynamics.com/*"],
            "js": ["content-script.js"],
            "run_at": "document_idle",
        }
    ],
}
